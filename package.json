{
  "name": "safecodechecker",
  "displayName": "safecodechecker",
  "description": "a vs code extension that checks if your code is safe",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.104.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": ["onCommand:safecodechecker.checkcode"],
  "main": "./src/testextension.ts",
  "contributes": {
    "commands": [
      {
        "command": "safecodechecker.checkcode",
        "title": "check for safe code"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.104.1",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "typescript-eslint": "^8.46.3",
    "eslint": "^9.39.1",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2"
  }
}
